generator client {
  provider = "dart run orm"
  output     = "../lib/_generated_prisma_client"
  engineType = "flutter"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
} 

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
}

model Macaco {
  id    Int     @id @default(autoincrement())
  cor String 
}